{
  "name": "bath-pride",
  "version": "0.0.1",
  "description": "Bath Pride website",
  "main": "www/index.html",
  "scripts": {
    "build": "vite build --emptyOutDir",
    "clear_jest": "jest --clearCache",
    "demo": "jest demo.ts --testMatch \\\"**/demo.ts\\\"",
    "demo:dev": "jest demo.ts --watch --testMatch \\\"**/demo.ts\\\"",
    "lint": "npm-run-all lint:prettier lint:eslint lint:stylelint",
    "lint:fix": "npm-run-all lint:prettier:fix lint:eslint:fix lint:stylelint:fix",
    "lint:eslint": "eslint \"**/*.tsx\" \"**/*.ts\"",
    "lint:eslint:fix": "eslint \"**/*.tsx\" \"**/*.ts\" --fix",
    "lint:prettier": "prettier --config .prettierrc.json --check **/*.tsx **/*.ts",
    "lint:prettier:fix": "prettier --config .prettierrc.json --write **/*.tsx **/*.ts",
    "lint:stylelint": "stylelint **/*.scss",
    "lint:stylelint:fix": "stylelint **/*.scss --fix",
    "start": "vite dev --host",
    "serve": "vite preview"
  },
  "author": "Greg Skinner",
  "license": "ISC",
  "dependencies": {
    "@eslint/compat": "^1.2.8",
    "classnames": "2.5.1",
    "date-fns": "^4.1.0",
    "normalize.css": "8.0.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router-dom": "7.5.1",
    "react-social-media-embed": "^2.5.18",
    "swiper": "^11.2.6",
    "typescript": "5.8.3",
    "vike": "0.4.228",
    "vite": "6.3.2"
  },
  "devDependencies": {
    "@types/jest": "29.5.14",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "@typescript-eslint/eslint-plugin": "8.30.1",
    "@typescript-eslint/parser": "8.30.1",
    "@vitejs/plugin-react": "4.4.0",
    "eslint": "9.25.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-import-resolver-typescript": "4.3.2",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "jest": "29.7.0",
    "npm-run-all": "4.1.5",
    "postcss-scss": "4.0.9",
    "prettier": "3.5.3",
    "react-test-renderer": "19.1.0",
    "sass": "1.86.3",
    "stylelint": "16.18.0",
    "stylelint-config-standard-scss": "14.0.0",
    "ts-jest": "29.3.2",
    "tslib": "2.8.1"
  }
}
