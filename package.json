{
  "name": "website",
  "version": "0.0.1",
  "description": "Powerful Wizard Games website",
  "main": "www/index.html",
  "scripts": {
    "build": "vite build",
    "clear_jest": "jest --clearCache",
    "demo": "jest demo.ts --testMatch \\\"**/demo.ts\\\"",
    "demo:dev": "jest demo.ts --watch --testMatch \\\"**/demo.ts\\\"",
    "lint": "npm-run-all lint:prettier lint:eslint lint:stylelint",
    "lint:fix": "npm-run-all lint:prettier:fix lint:eslint:fix lint:stylelint:fix",
    "lint:eslint": "eslint \"**/*.tsx\" \"**/*.ts\"",
    "lint:eslint:fix": "eslint \"**/*.tsx\" \"**/*.ts\" --fix",
    "lint:prettier": "prettier --config .prettierrc.json --check **/*.tsx **/*.ts",
    "lint:prettier:fix": "prettier --config .prettierrc.json --write **/*.tsx **/*.ts",
    "lint:stylelint": "stylelint **/*.scss",
    "lint:stylelint:fix": "stylelint **/*.scss --fix",
    "start": "vite dev",
    "serve": "vite preview"
  },
  "author": "Greg Skinner",
  "license": "ISC",
  "dependencies": {
    "@staticcms/proxy-server": "^3.0.2",
    "classnames": "^2.3.2",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "typescript": "^5.2.2",
    "vike": "^0.4.142",
    "vite": "^5.0.10"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.2.8",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.50.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "postcss-scss": "^4.0.9",
    "prettier": "^2.8.8",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.68.0",
    "stylelint": "^15.10.3",
    "stylelint-config-standard-scss": "^9.0.0",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2"
  }
}
